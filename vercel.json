{
  "version": 2,
  "builds": [
    {
      "src": "django_practice/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.12"
      }
    },
    {
      "src": "vercel.sh",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "assets/staticfiles"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "django_practice/wsgi.py"
    }
  ],
  "env": {
    "PYTHONPATH": "./",
    "DJANGO_SETTINGS_MODULE": "django_practice.settings",
    "SITE_DOMAIN": "https://iux-react-app.netlify.app"
  }
}